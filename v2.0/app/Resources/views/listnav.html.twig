<div class="listnav">
	{% if page > 1 %}
		<a href="{{ path(navRoute, {'page' : 1}) }}" class="btn"><<</a>
		<a href="{{ path(navRoute, {'page' : page - 1}) }}" class="btn"><</a>
	{% endif %}
		
	page : {{ page }} / {{ pageCount }}
	
	{% if page < pageCount %}
		<a href="{{ path(navRoute, {'page' : page + 1}) }}" class="btn">></a>
		<a href="{{ path(navRoute, {'page' : pageCount}) }}" class="btn">>></a>
	{% endif %}
</div>